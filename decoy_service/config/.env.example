# Environment variables for decoy service
# Copy this to .env and fill in your values

# Proxy settings (optional)
PROXY_LIST_FILE=
PROXY_ROTATION_ENABLED=false

# Browser settings
BROWSER_TIMEOUT=30
HEADLESS_MODE=true

# API endpoints (if using external randomization service)
RANDOM_WORD_API=https://random-word-api.herokuapp.com/all
RANDOM_IMAGE_API=https://picsum.photos/200/300

# Notification settings (optional)
DISCORD_WEBHOOK_URL=
SLACK_WEBHOOK_URL=

# Feature flags
DEBUG_MODE=false
DRY_RUN=false
